<div class="page-header"><h1>Welcome!</h1></div>
<% @categories.each do |category| %>
  <%= link_to category.title, category_path(category) %>
<% end %>

<% @items.each do |item| %>
  <p><%= item.title %> | <%= number_to_currency((item.price).to_f/100) %> |
    <% if user_signed_in? %>

      <%= button_to 'Add to Cart',line_items_path(item_id: item.id )%>

    <%end%>
    </p>
<% end %>
